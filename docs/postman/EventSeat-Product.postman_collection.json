                        "method": "GET",
                        "url": {
                            "raw": "{{gatewayBaseUrl}}/api/reviews/events/{{eventId}}/reviews",
                            "host": [
                                "{{gatewayBaseUrl}}"
                            ],
                            "path": [
                                "api",
                                "reviews",
                                "events",
                                "{{eventId}}",
                                "reviews"
                            ]
                        }
                    }
                },
                {
                    "name": "Create Review (ATTENDEE, attendance-gated)",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{attendeeToken}}"
                            },
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"eventId\": {{eventId}},\n  \"orderId\": {{orderId}},\n  \"rating\": 5,\n  \"text\": \"Fantastic event; well organized!\"\n}"
                        },
                        "url": {
                            "raw": "{{gatewayBaseUrl}}/api/reviews/reviews",
                            "host": [
                                "{{gatewayBaseUrl}}"
                            ],
                            "path": [
                                "api",
                                "reviews",
                                "reviews"
                            ]
                        }
                    }
                },
                {
                    "name": "Approve Review (ADMIN)",
                    "request": {
                        "method": "PATCH",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{adminToken}}"
                            }
                        ],
                        "url": {
                            "raw": "{{gatewayBaseUrl}}/api/reviews/reviews/{{reviewId}}/approve",
                            "host": [
                                "{{gatewayBaseUrl}}"
                            ],
                            "path": [
                                "api",
                                "reviews",
                                "reviews",
                                "{{reviewId}}",
                                "approve"
                            ]
                        }
                    }
                },
                {
                    "name": "Reject Review (ADMIN)",
                    "request": {
                        "method": "PATCH",
                        "header": [
                            {
                                "key": "Authorization",
                                "value": "Bearer {{adminToken}}"
                            }
                        ],
                        "url": {
                            "raw": "{{gatewayBaseUrl}}/api/reviews/reviews/{{reviewId}}/reject",
                            "host": [
                                "{{gatewayBaseUrl}}"
                            ],
                            "path": [
                                "api",
                                "reviews",
                                "reviews",
                                "{{reviewId}}",
                                "reject"
                            ]
                        }
                    }
                }
            ]
        }
    ]
}